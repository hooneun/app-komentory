		"@types/node": "^16.10.1",
		"@types/tailwindcss": "^2.2.1",
		"@vitejs/plugin-vue": "^1.9.2",
		"@vue/compiler-sfc": "^3.2.19",
		"autoprefixer": "^10.3.5",
		"postcss": "^8.3.8",
